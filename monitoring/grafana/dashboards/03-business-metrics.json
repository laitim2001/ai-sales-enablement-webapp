{
  "dashboard": {
    "title": "AI Sales Platform - Business Metrics",
    "uid": "ai-sales-business-metrics",
    "timezone": "browser",
    "editable": true,
    "panels": [
      {
        "id": 1,
        "title": "Total Users Registered (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(user_registrations_total[24h]))",
            "legendFormat": "New Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "blue"},
                {"value": 10, "color": "green"},
                {"value": 50, "color": "yellow"}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Active Users (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "expr": "count(count by (user_id) (user_activity_total{timestamp > (time() - 3600)}))",
            "legendFormat": "Active Users"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "AI Service Calls (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(ai_service_calls_total[24h]))",
            "legendFormat": "Total AI Calls"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "AI Token Usage (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(ai_tokens_used_total[24h]))",
            "legendFormat": "Tokens Used"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "percentage",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "red"}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "User Registrations Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(user_registrations_total[5m])) * 300",
            "legendFormat": "Registrations per 5min"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "New Users"},
          {"format": "short"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 6,
        "title": "User Login Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(user_logins_total{status=\"success\"}[5m])) * 300",
            "legendFormat": "Successful Logins"
          },
          {
            "expr": "sum(rate(user_logins_total{status=\"failed\"}[5m])) * 300",
            "legendFormat": "Failed Logins"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Logins per 5min"},
          {"format": "short"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4}
      },
      {
        "id": 7,
        "title": "Knowledge Base Search Queries",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(knowledge_base_searches_total[5m])) * 300",
            "legendFormat": "Total Searches"
          },
          {
            "expr": "sum(rate(knowledge_base_searches_total{has_results=\"true\"}[5m])) * 300",
            "legendFormat": "Searches with Results"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Searches per 5min"},
          {"format": "short"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12}
      },
      {
        "id": 8,
        "title": "AI Feature Usage by Type",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(increase(ai_service_calls_total[1h])) by (feature_type)",
            "legendFormat": "{{feature_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12}
      },
      {
        "id": 9,
        "title": "Dynamics 365 Sync Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(dynamics_sync_operations_total{status=\"success\"}[5m])) * 300",
            "legendFormat": "Successful Syncs"
          },
          {
            "expr": "sum(rate(dynamics_sync_operations_total{status=\"failed\"}[5m])) * 300",
            "legendFormat": "Failed Syncs"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Syncs per 5min"},
          {"format": "short"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 10,
        "title": "Sales Document Uploads",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(document_uploads_total[5m])) * 300",
            "legendFormat": "Total Uploads"
          },
          {
            "expr": "sum(rate(document_processing_complete_total[5m])) * 300",
            "legendFormat": "Processing Complete"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Documents per 5min"},
          {"format": "short"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20}
      },
      {
        "id": 11,
        "title": "AI Service Success Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "(sum(rate(ai_service_calls_total{status=\"success\"}[5m])) / sum(rate(ai_service_calls_total[5m]))) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 90, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 28}
      },
      {
        "id": 12,
        "title": "Average AI Response Time",
        "type": "gauge",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(ai_service_response_time_seconds_bucket[5m])) by (le))",
            "legendFormat": "P95 Response Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0,
            "max": 10,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 3, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        },
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 28}
      },
      {
        "id": 13,
        "title": "Token Usage vs Quota",
        "type": "bargauge",
        "targets": [
          {
            "expr": "(ai_tokens_used_total / ai_tokens_quota_total) * 100",
            "legendFormat": "Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "red"}
              ]
            }
          }
        },
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 28}
      },
      {
        "id": 14,
        "title": "Top 10 Active Users",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum(increase(user_activity_total[1h])) by (user_id))",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 28}
      },
      {
        "id": 15,
        "title": "Feature Adoption Rate (Last 7 Days)",
        "type": "table",
        "targets": [
          {
            "expr": "sum(increase(feature_usage_total[7d])) by (feature_name)",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 34}
      },
      {
        "id": 16,
        "title": "Customer Engagement Score",
        "type": "graph",
        "targets": [
          {
            "expr": "avg(customer_engagement_score)",
            "legendFormat": "Average Engagement"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Score", "min": 0, "max": 100},
          {"format": "short"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 34}
      }
    ],
    "refresh": "1m",
    "time": {"from": "now-24h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["30s", "1m", "5m", "15m", "30m", "1h", "2h"]
    },
    "version": 1
  }
}
