{
  "dashboard": {
    "title": "AI Sales Platform - API Performance",
    "uid": "ai-sales-api-performance",
    "timezone": "browser",
    "editable": true,
    "panels": [
      {
        "id": 1,
        "title": "API Request Rate by Endpoint",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m])) by (route)",
            "legendFormat": "{{route}}"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "API Status Code Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(increase(http_requests_total[1h])) by (status)",
            "legendFormat": "{{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Endpoint Response Time (P50, P95, P99)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, route))",
            "legendFormat": "{{route}} P50"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, route))",
            "legendFormat": "{{route}} P95"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, route))",
            "legendFormat": "{{route}} P99"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Response Time"},
          {"format": "short"}
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Error Rate by Endpoint (5xx)",
        "type": "graph",
        "targets": [
          {
            "expr": "(sum(rate(http_requests_total{status=~\"5..\"}[5m])) by (route) / sum(rate(http_requests_total[5m])) by (route)) * 100",
            "legendFormat": "{{route}}"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Error Rate %"},
          {"format": "short"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Client Error Rate (4xx)",
        "type": "graph",
        "targets": [
          {
            "expr": "(sum(rate(http_requests_total{status=~\"4..\"}[5m])) by (route) / sum(rate(http_requests_total[5m])) by (route)) * 100",
            "legendFormat": "{{route}}"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Error Rate %"},
          {"format": "short"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 6,
        "title": "Database Query Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(database_query_duration_seconds_bucket[5m])) by (le, query_type))",
            "legendFormat": "{{query_type}} P95"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Query Time"},
          {"format": "short"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 7,
        "title": "Database Connection Pool",
        "type": "graph",
        "targets": [
          {
            "expr": "database_connection_pool_active",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "database_connection_pool_idle",
            "legendFormat": "Idle Connections"
          },
          {
            "expr": "database_connection_pool_max",
            "legendFormat": "Max Connections"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Connections"},
          {"format": "short"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 8,
        "title": "Cache Hit Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "(sum(rate(cache_hits_total[5m])) / sum(rate(cache_requests_total[5m]))) * 100",
            "legendFormat": "Hit Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 70, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 32}
      },
      {
        "id": 9,
        "title": "Active WebSocket Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "websocket_connections_active",
            "legendFormat": "Active Connections"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 32}
      },
      {
        "id": 10,
        "title": "API Request Size Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_size_bytes_bucket[5m])) by (le))",
            "legendFormat": "P95 Request Size"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_response_size_bytes_bucket[5m])) by (le))",
            "legendFormat": "P95 Response Size"
          }
        ],
        "yaxes": [
          {"format": "bytes", "label": "Size"},
          {"format": "short"}
        ],
        "gridPos": {"h": 4, "w": 12, "x": 12, "y": 32}
      },
      {
        "id": 11,
        "title": "Top 10 Most Called Endpoints",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum(increase(http_requests_total[1h])) by (route, method))",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36}
      },
      {
        "id": 12,
        "title": "Top 10 Slowest Endpoints (P95)",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, route)))",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36}
      }
    ],
    "refresh": "30s",
    "time": {"from": "now-6h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "version": 1
  }
}
