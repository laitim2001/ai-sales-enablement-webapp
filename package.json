{
  "name": "ai-sales-enablement-webapp",
  "version": "0.1.0",
  "description": "AI-powered sales enablement platform for teams in Malaysia/Singapore",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:knowledge": "tsx e2e/run-knowledge-tests.ts",
    "test:e2e:knowledge:navigation": "tsx e2e/run-knowledge-tests.ts -- --suite navigation",
    "test:e2e:knowledge:main": "tsx e2e/run-knowledge-tests.ts -- --suite main",
    "test:e2e:knowledge:create": "tsx e2e/run-knowledge-tests.ts -- --suite create",
    "test:e2e:knowledge:upload": "tsx e2e/run-knowledge-tests.ts -- --suite upload",
    "test:e2e:knowledge:search": "tsx e2e/run-knowledge-tests.ts -- --suite search",
    "test:e2e:knowledge:details": "tsx e2e/run-knowledge-tests.ts -- --suite details",
    "test:e2e:knowledge:edit": "tsx e2e/run-knowledge-tests.ts -- --suite edit",
    "test:e2e:knowledge:performance": "tsx e2e/run-knowledge-tests.ts -- --suite performance",
    "test:integration": "tsx scripts/run-integration-tests.ts",
    "test:integration:crm": "tsx tests/integration/crm-integration.test.ts",
    "test:integration:system": "tsx tests/integration/system-integration.test.ts",
    "test:e2e:knowledge:integration": "tsx e2e/run-knowledge-tests.ts -- --suite integration",
    "test:e2e:knowledge:quick": "playwright test e2e/knowledge-base/navigation.spec.ts e2e/knowledge-base/main-page.spec.ts --workers=2",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose up",
    "docker:build": "docker build -f Dockerfile.prod -t ai-sales-enablement .",
    "docker:dev:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:prod:up": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "docker:prod:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "docker:prod:build": "docker-compose -f docker-compose.prod.yml build --no-cache",
    "services:health-check": "node scripts/health-check.js",
    "services:start": "docker-compose -f docker-compose.dev.yml up -d",
    "services:stop": "docker-compose -f docker-compose.dev.yml down",
    "services:restart": "npm run services:stop && npm run services:start",
    "index:check": "node scripts/check-index-sync.js",
    "index:check:incremental": "node scripts/check-index-sync.js --incremental",
    "index:check:auto-fix": "node scripts/check-index-sync.js --auto-fix",
    "index:update": "node scripts/index-maintenance.js",
    "index:watch": "node scripts/index-watcher.js",
    "index:health": "npm run index:check",
    "mvp:check": "node scripts/sync-mvp-checklist.js",
    "mvp:sync": "node scripts/sync-mvp-checklist.js",
    "dev:reminder": "node scripts/dev-reminder.js",
    "dev:start": "node scripts/dev-reminder.js --quiet && npm run dev",
    "poc:test": "cd poc && node run-all-tests.js",
    "commit": "npm run index:check:incremental && git add -A && git commit",
    "commit:safe": "npm run index:check && npm run lint && npm run type-check && git add -A && git commit",
    "precommit:index": "npm run index:check",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:deploy": "npm run db:generate && npm run db:migrate:deploy",
    "health-check": "node healthcheck.js",
    "deploy:staging": "echo 'Deploying to staging...' && npm run docker:prod:build && npm run docker:prod:up",
    "deploy:production": "echo 'Deploying to production..' && npm run db:deploy && npm run docker:prod:build && npm run docker:prod:up"
  },
